   <style>
      div {
        padding: 5px;
        margin:5px;
        width: 200px;
      }
   </style>
   
   <script>
   $(document).ready(function(){
      function selectTag()  {  
          jQuery('div').css("border","1px solid red");
      }  
   });
   </script>
</head>
<body>
 
   <input type="button" onclick="selectTag();" value="select Tag Name"/>
